<h1>bill</h1>
<section>
    {{#each model.customer as |customer|}}
    customer:<label>{{customer.customer}}</label>
    {{/each}}
</section>
<section>
        <table>
          <th>SI.NO</th>
          <th>Product</th> 
          <th>Quantity </th> 
          <th>COST(rs.)</th>
          <th>DELETE</th>
          {{#each model.bill as |bill|}}
          <tr>
             <td>{{bill.id}}</td>
             <td>{{bill.product}}</td>
             <td> {{bill.quantity }}</td>
             <td>{{bill.cost}}</td>
             <button  type="button">Remove</button>       
         </tr>
         {{/each}}
       </table>
    </section>
{{outlet}}